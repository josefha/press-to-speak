# Copy this file to .env for local development.
# Values from the process environment take precedence over .env values.

# Request timeout (seconds)
TRANSCRIPTION_REQUEST_TIMEOUT_SECONDS=45

# PressToSpeak API endpoint (used by both Account mode and BYOK mode).
# Example local API endpoint:
# TRANSCRIPTION_PROXY_URL=http://127.0.0.1:8787/v1/voice-to-text
TRANSCRIPTION_PROXY_URL=
TRANSCRIPTION_PROXY_API_KEY=

# Supabase client auth (required for PressToSpeak Account mode).
# Prefer SUPABASE_PUBLISHABLE_KEY. SUPABASE_ANON_KEY remains supported for compatibility.
SUPABASE_URL=
SUPABASE_PUBLISHABLE_KEY=
SUPABASE_ANON_KEY=

# Optional defaults forwarded to API provider parameters.
ELEVENLABS_MODEL_ID=scribe_v1

# Legacy direct-Transcription fallback settings (not used in primary flow).
ELEVENLABS_API_KEY=
ELEVENLABS_API_BASE_URL=https://api.elevenlabs.io
